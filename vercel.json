{
  "version": 2,
  "name": "global-food-databases",
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.pdf",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/report\\.html",
      "dest": "/report.html"
    },
    {
      "src": "/report\\.pdf",
      "dest": "/report.pdf"
    },
    {
      "src": "/$",
      "dest": "/index.html"
    }
  ]
}
